// Turn all the markdown files in `notes` into data ready for templating

// Turn the markdown in each note into HTML
Tree.map(notes, Origami.mdHtml)

-> (notesHtml) => Tree.map(notesHtml, (note, key) => {
  ...note
  date: new Date(note/date)
  path: `/notes/${ key }`
  type: "note"
})

-> (notesWithDates) => Tree.sort(notesWithDates, (note) => note/date)

-> Tree.reverse
