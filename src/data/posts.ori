// Turn all the markdown files in `posts` into data ready for templating

// Turn the markdown in each post into HTML
Tree.map(posts, Origami.mdHtml)

-> (postsHtml) => Tree.map(postsHtml, (post, key) => {
  ...post
  date: new Date(post/date)
  path: `/posts/${ key }`
  type: "post"
})

-> (postsWithDates) => Tree.sort(postsWithDates, (post) => post/date)

-> Tree.reverse
